def sa(a):
    if len(a)>=2:
        f=0
        f=a[0]
        a[0]=a[1]
        a[1]=f
    return a

def sb(b):
    if len(b)>=2:
        f=0
        f=b[0]
        b[0]=b[1]
        b[1]=f
    return b

def ss(a, b):
    return sa(a), sb(b)

def pa(sp1, sp2):
    sp1.insert(0, sp2[0])
    sp2 = sp2[1:]
    return sp1, sp2

def pb(sp1, sp2):
    sp2.insert(0, sp1[0])
    sp1 = sp1[1:]
    return sp1, sp2
